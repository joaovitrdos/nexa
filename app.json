{
  "expo": {
    "expo": {
      "web": {
        "bundler": "metro"
      }
    },
    "name": "Nexa",
    "slug": "Nexa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-transparent.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-transparent.png",
        "backgroundColor": "#171717"
      }
    },
    "web": {
      "favicon": "./assets/logo-transparent.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
